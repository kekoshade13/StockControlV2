<MudDialog>
    <DialogContent>
        <MudTextField T="string" Label="Nombre del equipo" @bind-Value="EquipoDto.NameEquip" />
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Text" OnClick="() => MudDialog.Cancel()">Cancelar</MudButton>
        <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="Submit">
            @(EquipoDto.Id_Equip != 0 ? "Editar" : "Crear")
        </MudButton>
    </DialogActions>
</MudDialog>